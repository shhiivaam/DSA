#include<bits/stdc++.h>

using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        int arr[n];
        for(int i=0;i<n;i++)
        {
            cin>>arr[i];
        }
      
        //for negative numbers
        for(int i=0;i<n;i++)
        {
            if(arr[i]<0)
            {
                cout<<arr[i]<<" ";
            }
        }
        
        //for zeros
        for(int i=0;i<n;i++)
        {
            if(arr[i]==0)
            {
                cout<<arr[i]<<" ";
            }
        }

        //for positive numbers
        for(int i=0;i<n;i++)
        {
            if(arr[i]>0)
            {
                cout<<arr[i]<<" ";
            }
        }
        cout<<endl;
    }
}
