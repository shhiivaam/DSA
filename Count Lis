#include<bits/stdc++.h>
using namespace std;
int n;
int rec(int i , int j, vector<int>&arr){
    if(i == n-1 && j == n-1) return 1;

    int ans = 0;
    ans = rec(i+1,j+1,arr);
    if(arr[i] > arr[j]){
        ans = max(ans,rec(i+1,j+1,arr));
    }
   return ans; 
}
void solve(){
cin>>n;
vector<int>v(n);
for(int i=0;i<n;i++) cin>>v[i];

cout<<rec(0,1,v)<<endl;

}
signed main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);cout.tie(0);
    int t;
    cin>>t;
    while(t--){
        solve();
    }
}
