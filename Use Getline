#include<bits/stdc++.h>
using namespace std;

void solution() {
    string s;
    getline(cin, s);
    for (auto c : s) {
        if (c == 92) {
            break;
        }
        cout << c;
    }
}

int main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0); cout.tie(0);
    solution();
    return 0;
}

