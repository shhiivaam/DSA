#include<bits/stdc++.h>
using namespace std;
int mod=1e9+7;
void solve(){
    int n;
    cin>>n;
   int v[n];
   for(auto i=0;i<n;i++){
       cin>>v[i];
   }
    int prod=1;
    for(auto i=0;i<n;i++){
        prod = (prod *v[i])%mod;
    }
    cout<<prod<<endl;
}
signed main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);cout.tie(0);
    int t;
    cin>>t;
    while(t--){
        solve();
    }
}
