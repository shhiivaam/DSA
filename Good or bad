#include<bits/stdc++.h>
using namespace std;

void solution() {
    string s; cin >> s;
    if ((s.find("010") != string::npos) || (s.find("101") != string::npos)) {
        cout << "Good" << endl;
    } else {
        cout << "Bad" << endl;
    }
}

int main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0); cout.tie(0);
    int t; cin >> t;
    while (t--) {
        solution();
    }
    return 0;
}
